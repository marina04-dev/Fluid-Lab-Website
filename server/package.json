{
  "name": "fluid-lab-server",
  "version": "1.0.0",
  "description": "Backend server for Fluid Lab research website",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seedData.js",
    "seed:dev": "cross-env NODE_ENV=development node scripts/seedData.js",
    "seed:prod": "cross-env NODE_ENV=production node scripts/seedData.js",
    "reset-db": "node scripts/resetDatabase.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "fluid",
    "mechanics",
    "research",
    "api",
    "express",
    "mongodb"
  ],
  "author": "Fluid Lab Research Team",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "cross-env": "^7.0.3",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
